include "/usr/share/X11/locale/ru_RU.UTF-8/Compose"

<Multi_key> <O> <X>                         : "☠" U2620 # SKULL AND CROSSBONES
<Multi_key> <asciicircum> <bar>             : "↑" U2191 # ARROW UP
<Multi_key> <bar> <asciicircum>             : "↑" U2191 # ARROW UP
<Multi_key> <bar> <v>                       : "↓" U2193 # ARROW DOWN
<Multi_key> <v> <bar>                       : "↓" U2193 # ARROW DOWN
<Multi_key> <asciicircum> <asciicircum> <J> : "☂" U2602 # UMBRELLA ^j^
<Multi_key> <dollar> <B>                    : "Ƀ" U0243 # Bitcoin

<Multi_key> <Cyrillic_BE> <Cyrillic_BE>     : "«"   guillemotleft # LEFT-POINTING DOUBLE ANGLE QUOTATION MARK
<Multi_key> <Cyrillic_YU> <Cyrillic_YU>     : "»"   guillemotright # RIGHT-POINTING DOUBLE ANGLE QUOTATION MARK
